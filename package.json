{
    "name": "goapi",
    "version": "1.0.0",
    "description": "A starting point for Node.js express apps with TypeScript",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
    },
    "author": "Golery",
    "license": "Private",
    "scripts": {
        "dev": "ts-node-dev --inspect=5010 --exit-child src/server.ts",
        "dev-proddb": "NODE_ENV=production ts-node-dev --exit-child src/server.ts",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "test": "ts-mocha tests/**/*.ts",
        "test:file": "ts-mocha",
        "test:e2e": "ts-mocha tests/e2e/**/*.ts",
        "test:router": "ts-mocha tests/router/**/*.ts",
        "test:grep": "ts-mocha tests/**/*.ts --grep",
        "test:debug": "mocha --require ts-node/register/transpile-only --inspect-brk=5000 tests/**/*.ts --grep",
        "fmt": "prettier --write src/**/*.ts test/**/*.ts",
        "start": "node --trace-deprecation dist/server.js"
    },
